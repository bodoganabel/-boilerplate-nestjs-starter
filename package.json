{
    "name": "nestjs-task-management",
    "version": "1.0.0",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "release-dockered": "npm run build && heroku container:login && npm run docker:build && heroku container:push web && heroku container:release web && heroku open",
        "release-dockered:clean": "npm run build && heroku container:login && npm run docker:build-clean && heroku container:push web && heroku container:release web && heroku open",
        "docker:build": "docker build --tag dockered-nestjs .",
        "docker:build-clean": "docker build --no-cache --tag dockered-nestjs .",
        "docker:inspect-image": "docker run -it dockered-nestjs sh",
        "docker:remove": "docker image rm dockered-nestjs",
        "docker:run": "docker run -p 3000:3000 dockered-nestjs .",
        "push:heroku-master": "git push heroku master",
        "heroku:terminal": "heroku run bash",
        "log:tail": "heroku logs --tail",
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start:dev": "cross-env STAGE=development nest start --watch",
        "start:debug": "cross-env STAGE=development nest start --debug --watch",
        "start:prod": "cross-env STAGE=production node dist/main",
        "start": "npm run start:prod",
        "db:dev:setup": "npm run db:mongodb:setup",
        "db:dev:start": "npm run db:mongodb:start",
        "db:dev:stop": "npm run db:mongodb:stop",
        "db:mongodb:setup": "docker-compose -f ./mongodb-docker/docker-compose.yml --env-file ./.env.stage.development up",
        "db:mongodb:start": "npm run db:mongodb:setup",
        "db:mongodb:stop": "docker-compose -f ./mongodb-docker/docker-compose.yml down",
        "db:dev:postgres:setup": "docker run --name postgres-nest -p 5432:5432 -e POSTGRES_PASSWORD=postgres -d postgres",
        "db:dev:postgres:start": "docker container start postgres-nest",
        "db:dev:postgres:stop": "docker container stop postgres-nest",
        "migrations:run": "npm run build && npx typeorm migration:run",
        "migrations:down": "npm run build && npx typeorm migration:revert",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
        "lint:fix": "eslint --quiet --fix --ext .ts,.js,.jsx .",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "build-commit-release": "git checkout master && npm run build && git add -f dist && git add -f .env.stage.production && git commit -m\"Push  dist\" && npm run push:heroku-master && git checkout dev"
    },
    "dependencies": {
        "@nestjs-modules/mailer": "^1.6.1",
        "@nestjs/common": "^8.0.0",
        "@nestjs/config": "^2.0.0",
        "@nestjs/core": "^8.0.0",
        "@nestjs/jwt": "^8.0.0",
        "@nestjs/mongoose": "^9.0.3",
        "@nestjs/passport": "^8.2.1",
        "@nestjs/platform-express": "^8.0.0",
        "@nestjs/schedule": "^1.1.0",
        "@nestjs/serve-static": "^2.2.2",
        "@nestjs/typeorm": "^8.0.3",
        "@types/bcrypt": "^5.0.0",
        "@types/cron": "^1.7.3",
        "@types/jsonwebtoken": "^8.5.8",
        "@types/passport-jwt": "^3.0.6",
        "@nestjs/cli": "^8.0.0",
        "bcrypt": "^5.0.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "cross-env": "^7.0.3",
        "ejs": "^3.1.8",
        "handlebars": "^4.7.7",
        "jsonwebtoken": "^8.5.1",
        "mongoose": "^6.2.10",
        "nodemailer": "^6.7.5",
        "passport": "^0.5.2",
        "passport-jwt": "^4.0.0",
        "pg": "^8.7.3",
        "puppeteer": "^14.1.1",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.2.0",
        "typeorm": "^0.2",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@nestjs/schematics": "^8.0.0",
        "@nestjs/testing": "^8.0.0",
        "@types/express": "^4.17.13",
        "@types/jest": "27.4.1",
        "@types/node": "^16.0.0",
        "@types/nodemailer": "^6.4.4",
        "@types/supertest": "^2.0.11",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^27.2.5",
        "prettier": "^2.3.2",
        "source-map-support": "^0.5.20",
        "supertest": "^6.1.3",
        "ts-jest": "^27.0.3",
        "ts-loader": "^9.2.3",
        "ts-node": "^10.0.0",
        "tsconfig-paths": "^3.10.1",
        "typescript": "^4.3.5"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}